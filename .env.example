# Environment Variables Example
# Copy this file to .env for development or .env.production for production

# API Base URL
VITE_API_URL=http://localhost:8000/api

# Sentry Configuration (Error Tracking)
# Get DSN from https://sentry.io after creating a project
# Format: https://[key]@[organization].ingest.sentry.io/[project-id]
VITE_SENTRY_DSN=

# Google Analytics 4
# Get Measurement ID from https://analytics.google.com
# Format: G-XXXXXXXXXX
VITE_GA_MEASUREMENT_ID=

# Enable GA in development (optional)
VITE_GA_DEBUG=false

# App Version (for Sentry releases)
VITE_APP_VERSION=1.0.0
